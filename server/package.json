{
  "name": "linksphere",
  "version": "1.0.0",
  "description": "This is a social networking webapp.",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "set NODE_ENV=development&& nodemon src/index.ts",
    "start:prod": "set NODE_ENV=production&& node dist/index.js",
    "prismaStudio:dev": "dotenv -e .env.development -- npx prisma studio",
    "prismaStudio:prod": "dotenv -e .env.production -- npx prisma studio",
    "prismaMigrate:dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "prismaMigrate:prod": "dotenv -e .env.production -- npx prisma migrate dev",
    "build": "tsc && cpy \"src/templates/**/*\" \"dist/templates\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "henzyd",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.8",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.3",
    "@types/nodemailer": "^6.4.8",
    "@types/nodemailer-express-handlebars": "^4.0.2",
    "@types/passport": "^1.0.13",
    "@types/passport-google-oauth20": "^2.0.12",
    "cpy-cli": "^4.2.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "prisma": "^4.14.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "^4.14.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "google-auth-library": "^8.9.0",
    "googleapis": "^122.0.0",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.9.3",
    "nodemailer-express-handlebars": "^6.1.0",
